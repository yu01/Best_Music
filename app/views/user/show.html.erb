<p id="notice"><%= notice %></p>

<!--<iframe width="560" height="315" src="http://www.youtube.com/embed/bmXumtgwtak?rel=0" frameborder="0" allowfullscreen></iframe>-->
<%= link_to 'New Post', user_add_path %>

 <% @posts.each do |post| %>

  <% if post.suit %>

    <table class="post">
      <tr> <td><%= post.title %> </td></tr>

      <tr><td><iframe width="370" height="210" src= "<%= post.url %>"  frameborder="0" allowfullscreen></iframe></td> </tr>

     <tr> <td> <%= post.tags %> </td>

      <td><%= post.vote %> </td> </tr>


    </table>

  <% end %>

 <% end %>


