<p id="notice"><%= notice %></p>


<%= link_to 'New Post', new_post_path %>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Url</th>
      <th>Tags</th>
      <th>Vote</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.url %></td>
        <td><%= post.tags %></td>
        <td><%= post.vote %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

